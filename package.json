{
  "name": "typed-option",
  "version": "3.2.0",
  "description": "Typed option library",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "lint": "prettier \"./{src,test}/**.ts\" --write",
    "build": "tsc -p .",
    "build:test": "tsc --noEmit -p ./test",
    "jest": "jest"
  },
  "keywords": [
    "option",
    "typed",
    "typescript",
    "functional"
  ],
  "author": "anderscan",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "codecov": "^3.0.2",
    "fast-check": "^1.1.3",
    "jest": "^23.0.0",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.6",
    "typescript": "2.8.3"
  },
  "repository": "AndersCan/typed-option",
  "jest": {
    "coverageDirectory": "./coverage/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
